{
  "name": "booking.calm",
  "private": true,
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "lint": "xo"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-xo": "^0.24.2",
    "lint-staged": "^7.2.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.2",
    "prettier-eslint": "^8.8.2",
    "xo": "^0.22.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add",
      "xo"
    ]
  },
  "precommit": {
    "run": [
      "lint-staged"
    ]
  },
  "xo": {
    "parser": "babel-eslint",
    "envs": [
      "browser",
      "node"
    ],
    "prettier": true,
    "space": true,
    "rules": {
      "camelcase": 0
    }
  }
}
